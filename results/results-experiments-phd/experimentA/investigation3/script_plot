set output 'simulation3_2_1.png'
set border 3
set pointintervalbox 3
plot "sgf_tournament.dat" u 3:6 t "SGF" with linespoint ls 3 lw 4 ps 3
EOFMarker
}


function g4 {
gnuplot -persist <<-EOFMarker
