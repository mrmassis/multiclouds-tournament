function g5 {
gnuplot -persist <<-EOFMarker
reset
set title "SATISFAÇÃO GLOBAL DA FEDERAÇÃO E DO TORNEIO\n Variação do índice quanto ao número de requisições" font "sans, 19"
set key outside right top vertical Left reverse noenhanced autotitle columnhead nobox
set key invert samplen 4 spacing 1 width 0 height 0

set yrange [0:1.5]
set xrange [0:380]
set grid y
set tics font "2"
set boxwidth 0.95
set ytics 0.5
set ylabel "SGF" font "sans, 20"
set xlabel "Número de Requisições na Federação" font "sans, 20"
set terminal pngcairo size 800,600
set output 'simulation3_1_complete.png'
set border 3
set pointintervalbox 3
plot "sgf_federation.dat" u 5:8 t "Federação" with linespoint ls 3 lw 4 ps 2, "sgf_tournament.dat" u 3:6 t "Torneio" with linespoint ls 2 lw 4 ps 2
EOFMarker
}

function g6 {
gnuplot -persist <<-EOFMarker
reset
set title "SATISFAÇÃO GLOBAL DA FEDERAÇÃO E DO TORNEIO\n Variação do índice quanto ao número de CSPs" font "sans, 19"
set key outside right top vertical Left reverse noenhanced autotitle columnhead nobox
set key invert samplen 4 spacing 1 width 0 height 0

set yrange [0:1.5]
set xrange [0:110]
set grid y
set tics font "2"
set boxwidth 0.95
set ytic 0.5
set ylabel "SGF" font "sans, 20"
set xlabel "Número de CSPs na Federação" font "sans, 20"
set terminal pngcairo size 800,600
set output 'simulation3_2_complete.png'
set border 3
set pointintervalbox 3
plot "sgf_federation.dat" u 2:8 t "Federação" with linespoint ls 3 lw 4 ps 2, "sgf_tournament.dat" u 2:6 t "Torneio" with linespoint ls 2 lw 4 ps 2,
EOFMarker
}

function g7 {
gnuplot -persist <<-EOFMarker
reset
set title "SATISFAÇÃO INDIVIDUAL NA FEDERAÇÃO \n Variação do Indíce quanto ao Número de Free-Riders" font "sans, 17"
set key outside right top vertical Left reverse noenhanced autotitle columnhead nobox
set key invert samplen 4 spacing 1 width 0 height 0

set style histogram columnstacked
set style histogram cluster gap 1
set style fill solid noborder
set yrange [0:1.1]
set xrange [0:100]

set grid ytics xtics mytics
set mytics 2
set grid
set tics font ", 7"
set boxwidth 0.95
set xtic scale 0.5
set ytic scale 0.5
set xtic 10
set ytic 0.5
set ylabel "SIs dos CSPs Conscientes" font "sans, 17"
set xlabel "Número de Free-Riders" font "sans, 17"
set terminal pngcairo size 800,600
set output "investigation3_1_SI.png"
set border 3
set pointintervalbox 3
plot "playerVirtual0_federation.dat" u 2:1 w p pt 1 lt 1 ps 1.5 t "CSP 0", "playerVirtual1_federation.dat" u 2:1 w p pt 2 lt 2 ps 1.5 t "CSP 1", "playerVirtual2_federation.dat" u 2:1 w p pt 3 lt 3 ps 1.5 t "CSP 2", "playerVirtual3_federation.dat" u 2:1 w p pt 4 lt 4 ps 1.5 t "CSP 3", "playerVirtual4_federation.dat" u 2:1 w p pt 5 lt 5 ps 1.5 t "CSP 4", "playerVirtual5_federation.dat" u 2:1 w p pt 6 lt 6 ps 1.5 t "CSP 5", "playerVirtual6_federation.dat" u 2:1 w p pt 7 lt 7 ps 1.5 t "CSP 6", "playerVirtual7_federation.dat" u 2:1 w p pt 8 lt 8 ps 1.5 t "CSP 7", "playerVirtual8_federation.dat" u 2:1 w p pt 9 lt 9 ps 1.5 t "CSP 8", "playerVirtual9_federation.dat" u 2:1 w p pt 10 lt 10 ps 1.5 t "CSP 9"
EOFMarker
}


function g8 {
gnuplot -persist <<-EOFMarker
reset
set title "SATISFAÇÃO INDIVIDUAL NO TORNEIO \n Variação do Indíce quanto ao Número de Free-Riders" font "sans, 17"
set key outside right top vertical Left reverse noenhanced autotitle columnhead nobox
set key invert samplen 4 spacing 1 width 0 height 0

set style histogram columnstacked
set style histogram cluster gap 1
set style fill solid noborder
set yrange [0:1.1]
set xrange [0:100]

set grid ytics xtics mytics
set mytics 2
set grid
set tics font ", 7"
set boxwidth 0.95
set xtic scale 0.5
set ytic scale 0.5
set xtic 10
set ytic 0.5
set ylabel "SIs dos CSPs Conscientes" font "sans, 17"
set xlabel "Número de Free-Riders" font "sans, 17"
set terminal pngcairo size 800,600
set output "investigation3_2_SI.png"
set border 3
set pointintervalbox 3
plot "playerVirtual0_tournament.dat" u 2:1 w p pt 1 lt 1 ps 1.5 t "CSP 0", "playerVirtual1_tournament.dat" u 2:1 w p pt 2 lt 2 ps 1.5 t "CSP 1", "playerVirtual2_tournament.dat" u 2:1 w p pt 3 lt 3 ps 1.5 t "CSP 2", "playerVirtual3_tournament.dat" u 2:1 w p pt 4 lt 4 ps 1.5 t "CSP 3", "playerVirtual4_tournament.dat" u 2:1 w p pt 5 lt 5 ps 1.5 t "CSP 4", "playerVirtual5_tournament.dat" u 2:1 w p pt 6 lt 6 ps 1.5 t "CSP 5", "playerVirtual6_tournament.dat" u 2:1 w p pt 7 lt 7 ps 1.5 t "CSP 6", "playerVirtual7_tournament.dat" u 2:1 w p pt 8 lt 8 ps 1.5 t "CSP 7", "playerVirtual8_tournament.dat" u 2:1 w p pt 9 lt 9 ps 1.5 t "CSP 8", "playerVirtual9_tournament.dat" u 2:1 w p pt 10 lt 10 ps 1.5 t "CSP 9"
EOFMarker
}


function g9 {
gnuplot -persist <<-EOFMarker
reset
set title "SATISFAÇÃO INDIVIDUAL MÉDIA \n Comparação entre uma Federação e um Torneio" font "sans, 20"
set key outside right top vertical Left reverse noenhanced autotitle columnhead nobox
set key invert samplen 4 spacing 1 width 0 height 0 
    
set style fill solid 1.0 noborder
set style histogram clustered
set style data histogram

lgray = "#DCDCDC"
dgray = "#808080"

set yrange [0:1]
set xrange [-0.1:10]
set grid y 
set tics font "2"
set boxwidth 0.95
set xtic scale 0.5
set ytic scale 0.5 
set ylabel "SI Média" font "sans, 20"
set xlabel "ID dos CSPs Conscientes" font "sans, 20"
set terminal pngcairo size 800,600
set output "experimentA_investigation3_SI_mean.png"
set border 3
set pointintervalbox 3

plot 'si_mean_federation_tournament.dat' using 2:xtic(1) title "Federação" linecolor rgb lgray, '' using 3 title "Torneio" linecolor rgb dgray
EOFMarker
}


function g10 {
gnuplot -persist <<-EOFMarker
reset
set title "SATISFAÇÃO INDIVIDUAL FINAL \n Comparação entre uma Federação e um Torneio"  font "sans, 20"
set key outside right top vertical Left reverse noenhanced autotitle columnhead nobox
set key invert samplen 4 spacing 1 width 0 height 0 
    
set style fill solid 1.0 noborder
set style histogram clustered
set style data histogram

lgray = "#DCDCDC"
dgray = "#808080"

set yrange [0:1]
set xrange [-0.1:10]
set grid y 
set tics font "2"
set boxwidth 0.95
set xtic scale 0.5
set ytic scale 0.5 
set ylabel "SI Final"  font "sans, 20"
set xlabel "ID dos CSPs Conscientes"  font "sans, 20"
set terminal pngcairo size 800,600
set output "experimentA_investigation3_SI_final.png"
set border 3
set pointintervalbox 3

plot 'si_final_federation_tournament.dat' using 2:xtic(1) title "Federação" linecolor rgb lgray, '' using 3 title "Torneio" linecolor rgb dgray
EOFMarker
}



#g5
#g6
#g7
#g8
g9
g10
